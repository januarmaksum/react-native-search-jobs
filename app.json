{
  "expo": {
    "name": "search-jobs",
    "slug": "search-jobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.januarmaksum.searchjobs"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "d29c23c3-b521-4ba7-b220-31d9af818196"
      }
    },
    "owner": "januarmaksum",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "scheme": "your-app-scheme"
  }
}
